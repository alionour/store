{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jasmine": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jasmine/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint",
        "jasmine"
    ],
    "rules": {
        "object-curly-spacing": "off"
    }
}